{
  "name": "@xarples/wolfi-vue",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/wolfi.ssr.js",
  "browser": "./dist/wolfi.esm.js",
  "module": "./dist/wolfi.esm.js",
  "unpkg": "./dist/wolfi.min.js",
  "types": "wolfi.d.ts",
  "files": [
    "dist/*",
    "wolfi.d.ts",
    "src/**/*.vue"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "build:watch": "cross-env NODE_ENV=production rollup --watch --config build/rollup.config.js",
    "docs:build": "cd ./docs && npm run build",
    "docs:dev": "vuepress dev docs"
  },
  "dependencies": {
    "@vue/composition-api": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-replace": "^2.2.1",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-typescript": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vuepress/plugin-register-components": "^1.3.1",
    "cross-env": "^6.0.3",
    "minimist": "^1.2.0",
    "prettier": "1.19.1",
    "rollup": "^1.27.13",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-css-only": "^2.0.0",
    "rollup-plugin-terser": "^5.1.3",
    "rollup-plugin-vue": "^5.1.5",
    "typescript": "^3.7.3",
    "vue": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^1.3.1"
  },
  "peerDependencies": {
    "vue": "^2.6.10"
  },
  "engines": {
    "node": ">=8"
  }
}
