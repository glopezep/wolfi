{
  "name": "@xarples/wolfi-react",
  "version": "1.0.0",
  "description": "Modern and Beutiful ui library",
  "author": "xarples",
  "license": "MIT",
  "repository": "xarples/wolfi-react",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
